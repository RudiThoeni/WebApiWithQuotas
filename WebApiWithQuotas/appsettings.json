{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimitConfig": [
    {
      "Type": "Anonymous",
      "TimeWindow": 60,
      "MaxRequests": 5
    },
    {
      "Type": "Referer",
      "TimeWindow": 60,
      "MaxRequests": 10
    },
    {
      "Type": "Logged",
      "TimeWindow": 60,
      "MaxRequests": 20
    }
  ]
}
